{
  "name": "@glazed/graphql",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-idx#readme",
  "keywords": [
    "ceramic",
    "glaze"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-idx",
    "directory": "packages/graphql"
  },
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/lib.mjs",
    "require": "./dist/lib.cjs",
    "default": "./dist/lib.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "del dist && tsc --emitDeclarationOnly --skipLibCheck && rollup -c ../../rollup.config.js",
    "lint": "eslint src test --fix",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@ceramicnetwork/stream-tile": "^1.1.1",
    "@glazed/append-collection": "^0.1.0",
    "@glazed/did-datastore": "^0.1.0",
    "@glazed/graphql-types": "^0.1.0",
    "change-case": "^4.1.2",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.8.0"
  },
  "devDependencies": {
    "@ceramicnetwork/common": "^1.2.1",
    "@ceramicnetwork/streamid": "^1.1.1",
    "@glazed/devtools": "^0.1.0",
    "jest-environment-glaze": "^0.1.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
