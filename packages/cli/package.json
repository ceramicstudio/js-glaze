{
  "name": "@glazed/cli",
  "description": "Glaze CLI",
  "version": "0.1.1",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-glaze#readme",
  "keywords": [
    "ceramic",
    "glaze",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-glaze",
    "directory": "packages/cli"
  },
  "main": "lib/index.js",
  "bin": {
    "glaze": "./bin/run"
  },
  "engines": {
    "node": ">=14.14"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint src",
    "postpack": "del oclif.manifest.json",
    "build": "del lib && tsc && cp src/index.js lib",
    "prepack": "yarn build && oclif-dev manifest",
    "test": " mocha --forbid-only \"test/**/*.test.ts\" --require ts-node/register",
    "test_coverage": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --require ts-node/register"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "glaze",
    "topics": {
      "config": {
        "description": "Manage local configuration details."
      },
      "did": {
        "description": "Manage local DIDs."
      },
      "model": {
        "description": "Interact with DataStore models."
      },
      "stream": {
        "description": "Interact with Ceramic Streams."
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@ceramicnetwork/3id-did-resolver": "^1.4.6",
    "@ceramicnetwork/http-client": "^1.4.2",
    "@ceramicnetwork/stream-tile": "^1.3.3",
    "@ceramicnetwork/streamid": "^1.3.1",
    "@glazed/datamodel": "^0.1.1",
    "@glazed/devtools": "^0.1.2",
    "@glazed/did-datastore": "^0.1.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.0",
    "cli-ux": "^5.6.3",
    "conf": "^10.0.3",
    "did-resolver": "^3.1.3",
    "dids": "^2.3.0",
    "env-paths": "^2.2.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.0",
    "jest-environment-glaze": "^0.1.1",
    "key-did-provider-ed25519": "^1.1.0",
    "key-did-resolver": "^1.4.0",
    "listr": "^0.14.3",
    "listr-update-renderer": "^0.5.0",
    "ora": "^5.3.0",
    "streamtest": "^2.0.0",
    "uint8arrays": "^3.0.0",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@ceramicnetwork/common": "^1.7.0",
    "@glazed/types": "^0.1.3",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/listr": "^0.14.4",
    "@types/node": "^16.11.6",
    "@types/update-notifier": "^5.1.0",
    "ajv": "^8.6.3",
    "get-stream": "^6.0.1",
    "globby": "^11.0.2",
    "mocha": "^5",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1"
  }
}
